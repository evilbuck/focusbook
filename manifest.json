{
  "manifest_version": 2,
  "name": "Focusbook",
  "description": "This plugin will tell you why you opened facebook",
  "version": "1.2",
  "permissions": [
    "https://*.facebook.com/*",
    "storage"
   ],
   "web_accessible_resources": [  
      "styles.css",
      "banner.html"
    ],
  "browser_action": {
    "default_icon" : "Group.png",
    "default_popup": "options.html"
  },
   "options_page": "options.html",
   "content_scripts": [
    {
      "matches": ["https://*.facebook.com/*"],
      "run_at": "document_end",
      "css": ["styles.css"],
      "js": ["lib/scheduler.js", "content_script.js"]
    }
  ]
}
